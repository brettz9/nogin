{
  "name": "nogin",
  "version": "1.0.0-beta.1",
  "description": "A template for quickly building login systems on top of Node.js",
  "author": "Brett Zamir",
  "contributors": [],
  "nyc": {
    "cache": false,
    "exclude": [
      ".eslintrc.js",
      ".ncurc.js",
      "nogin.js",
      "nogin-sample.js",
      "tools/mochawesome-cli.js",
      ".idea/**",
      "coverage/**",
      "instrumented/**",
      "cypress/**",
      "test/**",
      "mochawesome-report/**"
    ],
    "reporter": [
      "text",
      "json-summary",
      "lcov"
    ]
  },
  "scripts": {
    "license-badge": "license-badger --corrections --uncategorizedLicenseTemplate \"\\${license} (\\${name} (\\${version}))\" --filteredTypes=nonempty --textTemplate \"License types\n(project, deps, and bundled devDeps)\" --packageJson --production readme_includes/licenses-badge.svg",
    "license-badge-dev": "license-badger --corrections --filteredTypes=nonempty --textTemplate \"License types\n(all devDeps)\" --allDevelopment readme_includes/licenses-badge-dev.svg",
    "license-badges": "run-p license-badge license-badge-dev",
    "build-cli-svg": "clp --format=svg --config=app/server/optionDefinitions.js readme_includes/cli.svg",
    "build-cli-html": "clp --format=html --config=app/server//optionDefinitions.js readme_includes/cli.html",
    "build-cli": "run-p build-cli-svg build-cli-html",
    "cli": "node ./bin/cli.js",
    "help": "node ./bin/cli.js --help",
    "version": "node ./bin/cli.js --version",
    "eslint": "eslint --ext=js,md,html .",
    "lint": "npm run eslint",
    "start-open-report": "open http://localhost:3000/coverage/lcov-report/",
    "open-report": "run-p start start-open-report",
    "instrument-add": "nyc instrument app/public instrumented",
    "instrument-remove-cache": "rimraf node_modules/.cache",
    "instrument-remove-instrumented": "rimraf instrumented",
    "instrument-remove": "run-p -c instrument-remove-cache instrument-remove-instrumented",
    "instrument": "run-s -c instrument-remove instrument-add",
    "port": "sudo lsof -i :3000",
    "port-test": "sudo lsof -i :1234",
    "port-db": "sudo lsof -i :27017",
    "kill": "npm run stop-mongo",
    "stop-mongo": "kill -2 `pgrep mongo`",
    "mongo:start": "mongod --port=27017 --dbpath=db --bind_ip=127.0.0.1",
    "watch:server-start": "nodemon ./bin/cli.js -- --localScripts --config nogin.js --fromURL=https://braitsh.io --fromText=brettz9 --SERVE_COVERAGE",
    "server:start": "node ./bin/cli.js --localScripts --config nogin.js --fromURL=https://braitsh.io --fromText=brettz9 --SERVE_COVERAGE",
    "watch:server:start-production": "NODE_ENV=production npm run watch:server:start",
    "server:start-production": "NODE_ENV=production npm run server:start",
    "css-stylus": "stylus --sourcemap-inline app/public/css/style.styl",
    "css-copy": "cp -R ./app/public/css instrumented",
    "css": "run-p css-*",
    "unit-only": "nyc --all --require=esm --require=chai/register-expect mocha --reporter mochawesome --reporter-options reportDir=cypress/results,html=false,overwrite=true test/*.js --exit",
    "delayed-unit-only": "run-s delay unit-only",
    "unit": "run-p -r start-cov delayed-unit-only",
    "server:start-cov-only": "nyc --all node ./bin/cli.js --localScripts --config nogin.js --fromURL=https://braitsh.io --fromText=brettz9 --SERVE_COVERAGE -d instrumented",
    "server:start-cov": "run-s css server:start-cov-only",
    "server:delayed-start-cov": "run-s delay server:start-cov",
    "start-cov": "run-p -r mongo:start server:delayed-start-cov",
    "watch:start": "run-p -r mongo:start watch:server-start",
    "start": "run-p -r mongo:start server:start",
    "cypress:delayed-open-cov": "run-s -c delay cypress:open-cov",
    "cypress:delayed-open": "run-s -c delay cypress:open",
    "cypress:open-cov": "cypress open --env",
    "cypress:open": "cypress open --env coverage=false",
    "cypress:marge": "marge mochawesome.json",
    "cypress:merge-only": "mochawesome-merge \"cypress/results/mochaw*.json\" > mochawesome.json",
    "cypress:merge": "run-s cypress:merge-only cypress:marge test-badge",
    "cypress:remove-mochaawesome": "rimraf \"cypress/results/mochaw*.json\"",
    "cypress:remove-nyc-output": "rimraf \".nyc_output/*\"",
    "cypress:run-one": "cypress run --spec=\"cypress/integration/root.js\"",
    "cypress:run-only": "cypress run --spec=\"cypress/integration/*.js\"",
    "unit-and-cypress-run": "run-s -c delayed-unit-only cypress:run-only",
    "cypress:run-args": "npm run cypress:run-remove && cypress run",
    "cypress:run-remove": "run-p -c cypress:remove-mochaawesome cypress:remove-nyc-output",
    "cypress:run": "run-s -c cypress:run-remove unit-and-cypress-run cypress:merge",
    "cypress:run-no-unit": "run-s -c cypress:run-remove cypress:run-only cypress:merge",
    "delay": "delay 9",
    "cypress:delayed-run-no-unit": "run-s -c delay cypress:run-no-unit",
    "cypress:delayed-run": "run-s -c delay cypress:run",
    "start-open": "run-p -r start cypress:delayed-open",
    "start-open-cov": "run-p -r start-cov cypress:delayed-open-cov",
    "open-cov": "run-s -c instrument start-open-cov",
    "open-tests": "npm run open-cov",
    "watch:open": "run-p watch:start cypress:delayed-open",
    "open": "npm run start-open",
    "mochawesome-cli": "node tools/mochawesome-cli.js",
    "mochawesome-cli-dot": "npm run mochawesome-cli -- --dot",
    "report-no-mochawesome": "nyc report",
    "report-text-summary": "nyc report --reporter=text-summary",
    "report": "run-s report-no-mochawesome mochawesome-cli",
    "report-summary-mocha-full": "run-s report-text-summary mochawesome-cli",
    "report-summary": "run-s report-text-summary mochawesome-cli-dot",
    "test-cov-prereport": "run-p -r start-cov cypress:delayed-run",
    "test-cov-prereport-no-unit": "run-p -r start-cov cypress:delayed-run-no-unit",
    "clean-and-instrument": "run-p -c cypress:remove-nyc-output instrument",
    "coverage-badge": "coveradge readme_includes/coverage-badge.svg",
    "test-badge": "mbg --file ./mochawesome.json --badge_output readme_includes/tests-badge.svg",
    "test-cov": "run-s -c clean-and-instrument test-cov-prereport report-summary-mocha-full coverage-badge test-badge",
    "test-cov-no-unit": "run-s -c clean-and-instrument test-cov-prereport-no-unit report-summary-mocha-full",
    "test": "snyk test && run-p -r start cypress:delayed-run"
  },
  "engines": {
    "node": ">=10.4.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/brettz9/nogin"
  },
  "license": "MIT",
  "keywords": [
    "login"
  ],
  "main": "./app.js",
  "bin": {
    "nogin": "./bin/cli.js"
  },
  "homepage": "https://github.com/brettz9/nogin",
  "bugs": "https://github.com/brettz9/nogin/issues",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": "stop-only --file"
  },
  "dependencies": {
    "array-flat-polyfill": "^1.0.1",
    "body-parser": "^1.19.0",
    "bootstrap": "^4.4.1",
    "chai-as-promised": "^7.1.1",
    "command-line-basics": "^0.8.0",
    "connect-mongo": "^3.2.0",
    "cookie-parser": "^1.4.4",
    "emailjs": "^2.2.0",
    "express": "^4.17.1",
    "express-rate-limit": "^5.1.1",
    "express-session": "^1.17.0",
    "file-fetch": "^1.4.1",
    "font-awesome": "^4.7.0",
    "github-fork-ribbon-css": "^0.2.3",
    "intl-dom": "^0.5.0",
    "jamilih": "^0.51.0",
    "jquery": "^3.4.1",
    "jquery-form": "^4.2.2",
    "jsdom": "^16.2.1",
    "mongodb": "^3.5.4",
    "popper.js": "^1.16.1",
    "promise.allsettled": "^1.0.2",
    "stylus": "^0.54.7"
  },
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.8.3",
    "@babel/register": "^7.8.6",
    "@cypress/browserify-preprocessor": "^2.1.4",
    "@cypress/code-coverage": "^1.14.0",
    "@cypress/fiddle": "^1.4.2",
    "@fintechstudios/eslint-plugin-chai-as-promised": "^2.0.0",
    "@mysticatea/eslint-plugin": "^13.0.0",
    "acorn": "^7.1.1",
    "axe-core": "^3.5.2",
    "babel-eslint": "^10.1.0",
    "chai": "^4.2.0",
    "command-line-publish": "^0.7.0",
    "cookie-signature": "^1.1.0",
    "coveradge": "^0.3.0",
    "cypress": "^4.1.0",
    "cypress-axe": "^0.5.3",
    "cypress-multi-reporters": "^1.2.4",
    "delay-cli": "^1.1.0",
    "envelope": "^2.0.2",
    "eslint": "^6.8.0",
    "eslint-config-ash-nazg": "^17.4.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-array-func": "^3.1.4",
    "eslint-plugin-chai-expect": "^2.1.0",
    "eslint-plugin-chai-friendly": "^0.5.0",
    "eslint-plugin-compat": "^3.5.1",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-eslint-comments": "^3.1.2",
    "eslint-plugin-html": "^6.0.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsdoc": "^22.0.0",
    "eslint-plugin-markdown": "^1.0.2",
    "eslint-plugin-no-unsanitized": "^3.0.2",
    "eslint-plugin-no-use-extend-native": "^0.4.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-sonarjs": "^0.5.0",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-unicorn": "^17.1.0",
    "esm": "^3.2.25",
    "get-stream": "^5.1.0",
    "husky": "^4.2.3",
    "license-badger": "^0.11.1",
    "lint-staged": "^10.0.8",
    "mocha": "^7.1.0",
    "mocha-badge-generator": "^0.5.0",
    "mochawesome": "^5.0.0",
    "mochawesome-merge": "^4.0.2",
    "mochawesome-report-generator": "^4.1.0",
    "node-pop3": "^0.1.0",
    "nodemon": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.0.0",
    "rimraf": "^3.0.2",
    "snyk": "^1.298.1",
    "stop-only": "^3.1.0",
    "typescript": "^3.8.3"
  }
}
